	"scripts": {
		"compile": "yarn tsc --p tsconfig.json",
		"compile-server": "tsc --p tsconfig.server.json",
		"compile-test": "tsc --p tsconfig.test.json",
		"compile-watch": "yarn tsc --p tsconfig.json -w",
		"compile-server-watch": "yarn compile-server -w",
		"compile-test-watch": "yarn compile-test -w",
		"coverage": "nyc env NODE_ENV_PATH='/configTest.env' ./node_modules/.bin/istanbul cover _mocha -r \"./js/*.spec.js\" ",
		"test": "env NODE_ENV_PATH='/configTest.env' TZ='America/Sao_Paulo' ./node_modules/.bin/mocha --require ts-node/register --colors --exit --check-leaks --inspect --grep @all --watch-extensions spec.js './js/EntryPoint.spec.js' -R spec",
		"test-watch": "env NODE_ENV_PATH='/configTest.env' TZ='America/Sao_Paulo' ./node_modules/.bin/mocha --require ts-node/register --colors --exit --check-leaks --inspect --grep @all --watch-extensions spec.js './js/EntryPoint.spec.js' -R spec",
		"online": "NODE_ENV_PATH='./configTest.env' TZ='America/Sao_Paulo' node ./js/EntryPoint.js",
		"online-watch": "NODE_ENV_PATH='./configTest.env' TZ='America/Sao_Paulo' nodemon ./js/EntryPoint.js",
		"test-coverage": "nyc env NODE_ENV_PATH='/configTest.env' TZ='America/Sao_Paulo' ./node_modules/.bin/mocha --require ts-node/register --colors --exit --check-leaks --inspect --grep @all --watch-extensions spec.js './js/EntryPoint.spec.js' -R spec",
		"vulnerabilities": "nsp check --output summary && retire && snyk test",
		"tslintit": "tslint './ts/**/*.ts' ",
		"lint": "tslint ./ts/**/*.ts -t verbose",
		"lint-fix": "tslint --fix ./ts/**/*.ts -t verbose",
		"auto-db": "yarn sequelize-auto -h localhost -d hatchers6 -u postgres -x postgres -p 5432 -z true -C true -e postgres -o \"./models\" -c \"./sequelizeconfig.json\" -a \"./sequelizeconfig.json\"",
		"zip-deploy": "yarn clear-project && yarn shared-bring-uninstalled-to-root && ./zip-deploy.sh",
		"start": "yarn shared-install && yarn shared-compile && npm install && yarn compile-server && yarn server-start-deploy",
		"server-start-deploy": "NODE_ENV='production' NODE_ENV_PATH='../configProd.env' TZ='America/Sao_Paulo' node ./js/EntryPoint.js",
		"clear-project": "rm -rf node_modules js log coverage .nyc_output models extra-content salesfy-shared",
		"full-reinstall": "yarn clear-project && yarn shared-refresh && npm install && yarn compile",
		"shared-refresh": "yarn shared-bring-uninstalled-to-root && yarn shared-uninstall-as-module && yarn shared-install && yarn shared-compile && yarn shared-install-as-module",
		"shared-bring-uninstalled-to-root": "rm -rf ./salesfy-shared && rsync -av --progress ../salesfy-shared ./ --exclude node_modules --exclude js --exclude .git --exclude .npmrc --exclude .npmignore --exclude .gitignore",
		"shared-install": "cd ./salesfy-shared && npm install && cd ..",
		"shared-compile": "cd ./salesfy-shared && yarn compile-shared && cd ..",
		"shared-install-as-module": "npm install salesfy-shared@file:./salesfy-shared --save",
		"shared-uninstall-as-module": "npm uninstall salesfy-shared --save",
		"use-quick-register": "curl --data \"emUser=aaa@hatchers.com.br&unKeyPassword=12345678\" localhost:3001/user/register"
	},
	"nyc": {
		"extension": [
			".ts"
		],
		"exclude": [
			"**/*.spec.js",
			"**/*.d.ts"
		],
		"reporter": [
			"html",
			"text-summary",
			"text"
		],
		"require": [
			"ts-node/register"
		],
		"sourceMap": true,
		"instrument": true
	},
	"engines": {
		"npm": "5.6.0",
		"node": "9.11.2"
	}
